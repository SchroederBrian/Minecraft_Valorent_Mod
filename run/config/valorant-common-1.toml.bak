[curveball]
	#Maximum Curveball charges a player can hold at once.
	maxCharges = 2
	#Number of kills required to restore one Curveball charge.
	# Default: 2
	# Range: 1 ~ 10
	killRechargeThreshold = 2
	#Cooldown in ticks applied after throwing a Curveball.
	# Default: 20
	# Range: 0 ~ 600
	throwCooldownTicks = 20
	#Initial forward velocity applied to the Curveball orb.
	initialVelocity = 0.8
	#Distance in blocks the orb travels before beginning its curve.
	preCurveDistance = 2.5
	#Duration in ticks for the curved segment of the orb's flight.
	curveDurationTicks = 6
	#Total turn angle in degrees during the curve segment (left/right based on input).
	# Default: 90.0
	# Range: 10.0 ~ 180.0
	curveAngleDegrees = 90.0
	#Ticks after completing the curve before the orb detonates.
	# Default: 5
	# Range: 0 ~ 60
	detonationDelayTicks = 5
	#Maximum radius in blocks for Curveball to blind entities.
	# Default: 32.0
	# Range: 1.0 ~ 64.0
	flashRadius = 32.0
	#Field-of-view cone angle used to determine if entities are looking at the detonation (degrees).
	# Default: 130.0
	# Range: 30.0 ~ 180.0
	flashConeAngleDegrees = 130.0
	#If true, the thrower can be blinded by their own Curveball when facing it.
	affectsThrower = true

		#Flash timing: windup before flash, full-white hold, then slow fade
		[curveball.curveball.flashTiming]
			#Windup delay after detonation before flash starts (ticks)
			# Default: 10
			# Range: 0 ~ 100
			windupTicks = 10
			#Duration of full-white screen with no fade (ticks)
			# Default: 30
			# Range: 1 ~ 200
			fullTicks = 30
			#Fade-out duration after full-white completes (ticks)
			fadeTicks = 120

	[curveball.fireball]
		#Maximum Fireball charges a player can hold at once.
		# Default: 1
		# Range: 1 ~ 5
		maxCharges = 1
		#Number of kills required to restore one Fireball charge.
		# Default: 2
		# Range: 1 ~ 10
		killRechargeThreshold = 2
		#Cooldown in ticks applied after throwing a Fireball.
		# Default: 20
		# Range: 0 ~ 600
		throwCooldownTicks = 20
		#Initial forward velocity applied to the Fireball entity.
		# Default: 1.5
		# Range: 0.5 ~ 5.0
		initialVelocity = 1.5
		#Damage dealt by the Fireball explosion.
		# Default: 6.0
		# Range: 0.0 ~ 100.0
		damage = 6.0
		#Radius of the Fireball explosion.
		# Default: 3.0
		# Range: 1.0 ~ 10.0
		explosionRadius = 3.0

		[curveball.fireball.molly]
			#Radius of fire molly cloud.
			# Default: 3.5
			# Range: 0.5 ~ 12.0
			radius = 3.5
			#Duration of molly cloud in ticks.
			# Default: 200
			# Range: 20 ~ 600
			durationTicks = 200
			#Wither amplifier applied while in cloud (tick damage strength). 0=base.
			# Default: 0
			# Range: 0 ~ 4
			witherAmplifier = 0
			#Ticks before the cloud reapplies its effect to the same target.
			# Default: 10
			# Range: 1 ~ 40
			reapplicationDelay = 10
			#Direct damage dealt each tick interval inside the cloud.
			# Default: 1.0
			# Range: 0.0 ~ 20.0
			damagePerTick = 1.0
			#Interval in ticks between damage applications.
			# Default: 10
			# Range: 1 ~ 40
			tickInterval = 10
			#Primary particle id for molly (e.g., FLAME, CAMPFIRE_COSY_SMOKE, ASH, LAVA, SOUL_FIRE_FLAME)
			primaryParticle = "CAMPFIRE_COSY_SMOKE"
			#Secondary particle id for molly (optional, empty to disable)
			secondaryParticle = "FLAME"

	[curveball.agent]
		#Enable the agent selection menu system.
		selectionEnabled = true
		#Key binding code for opening agent selection menu.
		# Default: 77
		# Range: 0 ~ 255
		selectionKeyBinding = 77
		#Default agent ID to use when player has no selection.
		defaultAgent = "jett"
		#If true, each agent can be locked by at most one player per team.
		uniqueAgentsPerTeam = true
		#If true, the same agent can be locked once per team (mirror picks allowed).
		mirrorPicksAcrossTeams = true

	[curveball.hud]
		#Block all vanilla inventory/container screens (inventory, chests, crafting, etc.).
		blockAllVanillaInventories = true
		#Hide the vanilla hotbar to replace it with Valorant-style HUD.
		hideVanillaHotbar = true
		#Render the Valorant-style HUD overlay.
		showValorantHud = true
		#Global scale factor for the Valorant HUD (1.0 = 100%).
		# Default: 1.0
		# Range: 0.5 ~ 2.0
		hudScale = 1.0

		[curveball.hud.titleOverlay]
			#Ticks for title overlay to fade in.
			# Default: 10
			# Range: 0 ~ 100
			fadeInTicks = 10
			#Ticks for title overlay to stay visible.
			# Default: 700
			# Range: 0 ~ 400
			stayTicks = 400
			#Ticks for title overlay to fade out.
			# Default: 10
			# Range: 0 ~ 100
			fadeOutTicks = 10

		[curveball.weapons.classic]
			#Damage per shot for Classic pistol.
			# Default: 6.0
			# Range: 0.0 ~ 100.0
			damage = 6.0
			#Max range in blocks for Classic pistol.
			# Default: 1000.0
			# Range: 1000.0 ~ 1000.0
			range = 1000.0
			#Random spread in degrees for Classic pistol.
			# Default: 1.25
			# Range: 0.0 ~ 15.0
			spreadDegrees = 1.25
			#Cooldown (fire rate) in ticks for Classic pistol.
			# Default: 8
			# Range: 0 ~ 40
			cooldownTicks = 8
			#Tracer particle steps for Classic pistol (visual only).
			# Default: 5
			# Range: 5 ~ 5
			tracerSteps = 5
			#Muzzle particle count for Classic pistol (visual only).
			# Default: 4
			# Range: 1 ~ 32
			muzzleParticles = 4
			#Magazine size for Classic pistol.
			# Default: 12
			# Range: 1 ~ 100
			magazineSize = 12
			#Maximum reserve ammo for Classic pistol.
			# Default: 36
			# Range: 0 ~ 500
			maxReserveAmmo = 36
			#Reload time in ticks for Classic pistol (40 ticks = 2.0 seconds).
			# Default: 40
			# Range: 1 ~ 600
			reloadTimeTicks = 40

		[curveball.weapons.ghost]
			#Damage per shot for Ghost pistol.
			# Default: 8.0
			# Range: 0.0 ~ 100.0
			damage = 8.0
			#Max range in blocks for Ghost pistol.
			# Default: 64.0
			# Range: 1.0 ~ 256.0
			range = 64.0
			#Random spread in degrees for Ghost pistol.
			# Default: 0.9
			# Range: 0.0 ~ 15.0
			spreadDegrees = 0.9
			#Cooldown (fire rate) in ticks for Ghost pistol.
			# Default: 7
			# Range: 0 ~ 40
			cooldownTicks = 7
			#Tracer particle steps for Ghost pistol (visual only).
			# Default: 18
			# Range: 1 ~ 128
			tracerSteps = 18
			#Muzzle particle count for Ghost pistol (visual only).
			# Default: 5
			# Range: 1 ~ 32
			muzzleParticles = 5
			#Magazine size for Ghost pistol.
			# Default: 15
			# Range: 1 ~ 100
			magazineSize = 15
			#Maximum reserve ammo for Ghost pistol.
			# Default: 45
			# Range: 0 ~ 500
			maxReserveAmmo = 45
			#Reload time in ticks for Ghost pistol (45 ticks = 2.25 seconds).
			# Default: 45
			# Range: 1 ~ 600
			reloadTimeTicks = 45

		[curveball.weapons.vandal_rifle]
			#Damage per shot for Vandal Rifle.
			# Default: 5.0
			# Range: 0.0 ~ 100.0
			damage = 5.0
			#Max range in blocks for Vandal Rifle.
			# Default: 80.0
			# Range: 1.0 ~ 256.0
			range = 80.0
			#Random spread in degrees for Vandal Rifle.
			# Default: 0.75
			# Range: 0.0 ~ 15.0
			spreadDegrees = 0.75
			#Cooldown (fire rate) in ticks for Vandal Rifle.
			# Default: 4
			# Range: 0 ~ 40
			cooldownTicks = 4
			#Tracer particle steps for Vandal Rifle (visual only).
			# Default: 24
			# Range: 1 ~ 128
			tracerSteps = 24
			#Muzzle particle count for Vandal Rifle (visual only).
			# Default: 6
			# Range: 1 ~ 32
			muzzleParticles = 6
			#Magazine size for Vandal Rifle.
			# Default: 25
			# Range: 1 ~ 100
			magazineSize = 25
			#Maximum reserve ammo for Vandal Rifle.
			# Default: 75
			# Range: 0 ~ 500
			maxReserveAmmo = 75
			#Reload time in ticks for Vandal Rifle (50 ticks = 2.5 seconds).
			# Default: 50
			# Range: 1 ~ 600
			reloadTimeTicks = 50

		[curveball.commands.particles]
			#Default duration for /valorant particle command (ticks)
			# Default: 200
			# Range: 1 ~ 12000
			defaultDurationTicks = 200

	[curveball.spike]
		#Runtime flag set to true when Spike is planted. Server-updated.
		planted = false
		#Vertical offset applied to the planted spike ArmorStand (negative sinks it).
		# Default: -1.6
		# Range: -1.6 ~ -1.6
		plantedSpikeYOffset = -1.6

	[curveball.teams]
		#Maximum players per team (A and V).
		# Default: 5
		# Range: 1 ~ 10
		maxTeamSize = 5

	[curveball.firewall]
		#Maximum Fire Wall charges a player can hold at once.
		# Default: 1
		# Range: 1 ~ 5
		maxCharges = 1
		#Number of kills required to restore one Fire Wall charge.
		# Default: 3
		# Range: 1 ~ 10
		killRechargeThreshold = 3
		#Cooldown in ticks applied after using Fire Wall.
		# Default: 20
		# Range: 0 ~ 600
		throwCooldownTicks = 20
		#Maximum range in blocks for Fire Wall.
		# Default: 18.0
		# Range: 18.0 ~ 18.0
		maxRange = 18.0
		#Speed at which wall segments are placed (ticks per segment).
		# Default: 0.1
		# Range: 0.1 ~ 0.1
		growthSpeed = 0.1
		#Distance between wall segments in blocks.
		# Default: 0.5
		# Range: 0.5 ~ 0.5
		segmentSpacing = 0.5
		#How sensitive the wall is to player head movement (degrees per tick).
		# Default: 0.1
		# Range: 0.1 ~ 0.1
		curveSensitivity = 0.1
		#Duration in ticks that the fire wall lasts.
		# Default: 300
		# Range: 20 ~ 1200
		durationTicks = 300
		#Damage dealt per tick to entities touching the wall.
		# Default: 2.0
		# Range: 0.0 ~ 10.0
		damagePerTick = 2.0
		#Particle type for fire wall effect.
		particleType = "FLAME"
		#Vertical offset added to each wall segment position (blocks).
		# Default: -1.8
		# Range: -1.8 ~ -1.8
		yOffset = -1.8
		#Additional yaw rotation (degrees) applied to each wall segment.
		# Default: 0.0
		# Range: -180.0 ~ 180.0
		rotationOffsetDegrees = 0.0

	[curveball.reloadAnimation]
		#Enable first-person reload animations for both hands.
		enabled = true
		#Amount of hand swing/movement during reload animation.
		# Default: 0.3
		# Range: 0.0 ~ 2.0
		handSwingAmount = 0.3
		#Scale of the magazine item shown during reload animation.
		# Default: 0.8
		# Range: 0.1 ~ 2.0
		magazineScale = 0.8
		#Movement amplitude of the left hand during magazine swap.
		# Default: 0.5
		# Range: 0.0 ~ 2.0
		leftHandMovement = 0.5
		#Show ejected magazine during reload animation.
		showEjectedMagazine = true
		#Distance the ejected magazine flies out.
		# Default: 0.8
		# Range: 0.1 ~ 2.0
		ejectedMagazineOffset = 0.8
		#Enable slide racking animation at the end of reload.
		rackSlide = true
		#How far back the slide is pulled during racking.
		# Default: 0.15
		# Range: 0.0 ~ 0.5
		rackSlideAmount = 0.15
